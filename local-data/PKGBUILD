pkgname=local-data
pkgver=0.0.8
pkgrel=1
epoch=
pkgdesc="File for archiso"
arch=('any')
url=""
license=('GPL')
groups=(local-built)
depends=()
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
changelog=
source=("$pkgname.tar.gz")
noextract=()
sha256sums=('SKIP')
validpgpkeys=()

package() {
	mkdir -p "$pkgdir/usr/share/local-data/"
	mv Code "$pkgdir/usr/share/local-data/"
	mv Wallpaper "$pkgdir/usr/share/local-data/"
	mv doc "$pkgdir/usr/share/local-data/"
	chmod g+rw -R "$pkgdir/usr/share/local-data/"
}
